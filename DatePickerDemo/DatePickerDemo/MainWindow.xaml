<Window x:Class="DatePickerDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatePickerDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DatePicker
            x:Name="myDatePicker"
            SelectedDate="02.28.2022"
            SelectedDateFormat="Long"
            SelectedDateChanged="myDatePicker_SelectedDateChanged"
            >
            <DatePicker.BlackoutDates>
                <CalendarDateRange Start="02.02.2022" End="02.11.2022"></CalendarDateRange>
            </DatePicker.BlackoutDates>
        </DatePicker>
        
    </Grid>
</Window>
